RewriteEngine On# 1. Remove visualmente o .html da URL (Redireciona para sem .html)RewriteCond %{THE_REQUEST} \s/+(.+?)\.html[\s?] [NC]RewriteRule ^ /%1 [R=301,L,NE]# 2. Faz o servidor encontrar o arquivo .html internamenteRewriteCond %{REQUEST_FILENAME} !-dRewriteCond %{REQUEST_FILENAME}.html -fRewriteRule ^(.+?)/?$ $1.html [L]