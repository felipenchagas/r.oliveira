# Ativa o módulo de reescrita de URLsRewriteEngine OnOptions +FollowSymlinks# Redireciona URLs que contêm '/index.html' para a URL baseRewriteCond %{THE_REQUEST} \ /(.+/)?index(\.html)?(\?.*)?\  [NC]RewriteRule ^(.+/)?index(\.html)?$ /%1 [R=301,L]# Remove a extensão .html das URLsRewriteCond %{ENV:REDIRECT_STATUS} ^$RewriteRule ^(.+)\.html$ /$1 [R=301,L]# Adiciona a extensão .html se o arquivo existirRewriteCond %{SCRIPT_FILENAME}.html -fRewriteRule [^/]$ %{REQUEST_URI}.html [QSA,L]# Redireciona de www para não-www<IfModule mod_rewrite.c>    RewriteCond %{HTTP_HOST} ^www\.oliveiraalpinismo\.com\.br [NC]    RewriteRule (.*) https://oliveiraalpinismo.com.br/$1 [L,R=301]</IfModule># Configura os cabeçalhos de expiração para arquivos estáticos<FilesMatch "\.(ico|pdf|flv|jpg|jpeg|png|gif|js|css|swf)(\.gz)?$">    Header set Expires "Thu, 15 Apr 2025 20:00:00 GMT"    Header unset ETag    FileETag None</FilesMatch># Ativa e configura o módulo de expiração<IfModule mod_expires.c>    ExpiresActive On    ExpiresDefault "access plus 1 year"    # Configura expiração personalizada para diferentes tipos de arquivos    ExpiresByType text/css "access plus 1 year"    ExpiresByType text/javascript "access plus 1 year"    ExpiresByType application/javascript "access plus 1 year"    ExpiresByType image/gif "access plus 1 year"    ExpiresByType image/png "access plus 1 year"    ExpiresByType image/jpeg "access plus 1 year"    ExpiresByType application/json "access plus 1 year"    ExpiresByType application/pdf "access plus 1 year"    ExpiresByType image/bmp "access plus 1 year"    ExpiresByType image/svg+xml "access plus 1 year"    ExpiresByType font/woff "access plus 1 year"    ExpiresByType font/woff2 "access plus 1 year"</IfModule>