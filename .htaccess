RewriteEngine OnRewriteBase /# 1. For√ßar HTTPSRewriteCond %{HTTPS} offRewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]# 2. Remover .html da URL (Visual)RewriteCond %{THE_REQUEST} \s/+(.+?)\.html[\s?] [NC]RewriteRule ^ /%1 [R=301,L,NE]# 3. Adicionar .html internamente (Funcional)RewriteCond %{REQUEST_FILENAME} !-dRewriteCond %{REQUEST_FILENAME}.html -fRewriteRule ^(.+)$ $1.html [L,QSA]