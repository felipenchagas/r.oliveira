RewriteEngine OnRewriteBase /# 1. Se o usuário digitar "pagina.html", redireciona para "pagina" (Visual)RewriteCond %{THE_REQUEST} \s/+(.+?)\.html[\s?] [NC]RewriteRule ^ /%1 [R=301,L,NE]# 2. Se o usuário acessar "pagina", o servidor abre "pagina.html" (Interno)RewriteCond %{REQUEST_FILENAME} !-dRewriteCond %{REQUEST_FILENAME}.html -fRewriteRule ^(.*)$ $1.html [L]